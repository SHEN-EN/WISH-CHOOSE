<!--pages/result/result.wxml-->
<view class="DrawResult">
  <view class="Image" hover-class="none" hover-stop-propagation="false">
    <view >
      <image class="Cards" src="{{Cards}}"></image>
    </view>
    <view >
      <image class="wordCards" src="{{wordCards}}"></image>
    </view>
  </view>
  <view class="button">
    <button class="diary" bindtap="selectDiary">  
      <text>觉察日记</text>  
      <image class="diary_icon" src="https://shen-1259805780.cos.ap-chengdu.myqcloud.com/page_image/result/pencli.png"></image>
    </button>
    <button class="consult">
      <text>寻求咨询</text> 
      <image class="consult_icon" src="https://shen-1259805780.cos.ap-chengdu.myqcloud.com/page_image/result/backle.png"></image> 
    </button>
  </view>
</view>